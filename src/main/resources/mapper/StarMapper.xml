<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uchain.cip.mapper.StarMapper">


    <insert id="addThing">
        insert into star(id,user_id,thing_id,start_data_time,thing_type) values (#{id},#{user_id},#{thing_id},#{start_data_time},#{thing_type});
    </insert>
</mapper>
